SELECT profiles.user_id FROM profiles, users WHERE 1 = 1 AND (profiles.gender_id = 2) AND (profiles.seeking_id = 4 OR profiles.seeking_id = 8) AND (profiles.status_id = 1 OR profiles.status_id = 2 OR profiles.status_id = 4) AND (profiles.body_id = 1 OR profiles.body_id = 2 OR profiles.body_id = 4 OR profiles.body_id = 8 OR profiles.body_id = 16) AND (profiles.hair_id = 1 OR profiles.hair_id = 2 OR profiles.hair_id = 4 OR profiles.hair_id = 32) AND users.user_id = profiles.user_id AND users.user_type <> 'd' ORDER BY users.last_access DESC LIMIT 200


